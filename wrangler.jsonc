{
  "name": "merchant",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-01",

  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "merchant-db",
      "database_id": "local"
    }
  ],

  "r2_buckets": [
    {
      "binding": "IMAGES"
    }
  ],

  "vars": {
    "IMAGES_URL": ""
  },

  "triggers": {
    "crons": ["*/15 * * * *"]
  },

  "dev": {
    "port": 8787
  }
}
